<?xml version="1.0" encoding="UTF-8"?>

<config>
    <general> 
        <session>
            <user-session>user</user-session>
        </session>
    
        <title><![CDATA[Robomotic]]></title>
    
        <lang>
            <available-langs>it,en</available-langs>
            <default>it</default>
        </lang>
        
        <theme>
            <base-folder>themes</base-folder>
            <default>main</default>
        </theme>
    </general>
    
    <sensor-data>
        <max-queue-length>30</max-queue-length>
    </sensor-data>
    
    <persistence>
        <persistence-unit>robomotic</persistence-unit>
    </persistence>
       
</config>
